{
    "name": "clitch-discord-notification",
    "version": "1.0.0",
    "description": "Discord bot for sending user notifications",
    "keywords": [
        "clitch",
        "typescript",
        "discord",
        "bot"
    ],
    "homepage": "https://github.com/clitch-dev/clitch-discord-notification",
    "bugs": {
        "url": "https://github.com/clitch-dev/clitch-discord-notification/issues"
    },
    "author": {
        "name": "Hoon Kim",
        "url": "https://github.com/hoonsubin"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/clitch-dev/clitch-discord-notification.git"
    },
    "main": "src/index.ts",
    "scripts": {
        "start": "ts-node-dev -r dotenv/config src/index.ts",
        "build": "tsc --project tsconfig.json",
        "lint": "eslint '*/**/*.{js,ts,tsx}' --quiet --fix",
        "test": "NODE_ENV=test jest --setupFiles dotenv/config --verbose --coverage && eslint '*/**/*.{js,ts,tsx}'",
        "sandbox": "ts-node -r dotenv/config scripts/sandbox.ts"
    },
    "engines": {
        "node": ">=12.x"
    },
    "license": "MIT",
    "devDependencies": {
        "@types/node": "^14.14.14",
        "@typescript-eslint/eslint-plugin": "^4.6.0",
        "@typescript-eslint/parser": "^4.6.0",
        "dotenv": "^8.2.0",
        "eslint": "^7.12.0",
        "eslint-config-prettier": "^6.14.0",
        "eslint-plugin-prettier": "^3.1.4",
        "jest": "^26.6.1",
        "jest-config": "^26.6.1",
        "prettier": "^2.1.2",
        "ts-jest": "^26.4.2",
        "ts-node-dev": "^1.1.1",
        "typescript": "^4.1.3"
    },
    "dependencies": {
        "discord.js": "^12.5.1",
        "node": "^15.4.0",
        "ts-node": "^9.1.1"
    }
}
